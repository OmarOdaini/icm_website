<div class="container-fluid" style="height:1200px;">


    <!-- Add style to <p> -->

    <h5 class="MainTitle">MOSQUE HISTORY</h5>
    <div class="jumbotron" style="padding:15px; padding-bottom:10px;">
        @for (int i = 0; i < @ViewBag.para.GetLength(1); i++)
        {
            if (@ViewBag.para[0, i, 0] == null) { break; }

            @:<h6 style="margin:0px; font-family:Georgia;" class="Paratitle">@ViewBag.para[0, i, 0]</h6>
            @:<p style="font-size:1.2em;">
                @ViewBag.para[0, i, 1]
            @: </p>
        }
    </div>
</div>